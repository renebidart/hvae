{
    "name": "NotHVAE_L32_L32_rand_size_loc_fill_g8",
    "n_gpu": 1,
    
    "arch": {
        "type": "NotHVAE",
        "args": {
                 "img_size": 256,
                  "in_channels": 1,
                  "l2_latent_size": 32, 
                  "l1_latent_size": 32, 
                  "l2_stride": 32
        }
    },
    "data_loader": {
        "type": "make_gen_grid",
        "args":{
            "files_dict_loc": "/media/rene/data/hvae/shapes/sz32_rand_size_loc_fill_g8/files_dict.pkl",
            "batch_size": 128,
            "num_workers": 4,
            "img_size": 256
        }  
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 5e-4
        }
    },

    "loss": {
        "type": "make_vae_loss",
        "args":{
            "KLD_weight": 1
        }
    },
    "metrics": [
        "BCE", "KLD"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 100,
            "gamma": 0.2
        }
    },
    "trainer": {
        "epochs": 500,
        "save_dir": "/media/rene/data/hvae/shapes_output",
        "save_period": 1,
        "verbosity": 1,
        "give_model_label": false,
        
        "monitor": "min val_loss",
        "early_stop": 200,
        
        "tensorboardX": true,
        "log_dir": "/media/rene/data/hvae/shapes_output"
    }
}
